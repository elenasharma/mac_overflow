<h1>Sign Up</h1>
<%=form_for User.new do |f| %>
<%=f.label :username %>
<%=f.text_field :username %>
<%=f.label :password %>
<%=f.password_field :password %>
<%=f.label :password_confirmation %>
<%=f.password_field :password_confirmation %>
<%=f.submit "Sign Up" %>
<%end%>

<h1>Log In</h1>
<%=form_tag '/log_in' do%>
<%=label_tag :username %>
<%=text_field_tag :username %>
<%=label_tag :password %>
<%=password_field_tag :password %>
<%=submit_tag "Log in" %>
<%end%>