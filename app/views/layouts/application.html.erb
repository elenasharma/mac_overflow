<!DOCTYPE html>
<html>
<head>
  <title>MacOverflow</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="background">
  </div>
  <%= render partial: 'layouts/signin' %>
  <%= render partial: 'layouts/login' %>
  <header class="layout_header">
    <% if !current_user %>
      <a class="nav_links login_link">Login</a>
      <a class="nav_links signin_link">Sign Up</a>
    <% else %>
      <%= link_to "Sign Out", log_out_path, class: "nav_links sign_out" %>
    <% end %>
    <h1 class="title"><%= link_to "Mac Overflow", questions_path %></h1>
  </header>

<%= yield %>

</body>
</html>
